{
  "manifest_version": 2,
  "name": "Clockify",
  "short_name": "Clockify'",
  "version": "1.0.0",
  "description": "Chrome Extension for Clockify",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_title": "Chrome Extension Clockify",
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "options_page": "options.html"
}
